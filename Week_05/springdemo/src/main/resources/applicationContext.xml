<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <bean id="xml111" class="fx.encin.springdemo.Student">
        <property name="id" value="111"/>
        <property name="name" value="XML1"/>
    </bean>

    <bean id="xml222" class="fx.encin.springdemo.Student">
        <property name="id" value="222"/>
        <property name="name" value="XML2"/>
    </bean>

    <context:component-scan base-package="fx.encin.springdemo"/>

    <bean id="beanAnnoDemo" class="fx.encin.springdemo.BeanAnnoDemo"></bean>
</beans>